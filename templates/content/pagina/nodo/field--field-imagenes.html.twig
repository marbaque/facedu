<div class="slick__carousel">
        {% for item in items%}
            <div class="content">
                <div class="pagina__imagen-contenedor">
                    {{ item.content }}
                </div>
                <div class="pagina__descripcion">
                    <p>
                        {{ item.content['#item'].alt }}
                    </p>
                </div>
            </div>
        {% endfor %}
</div>